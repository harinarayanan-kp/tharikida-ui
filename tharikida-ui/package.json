{
  "name": "tharikida-ui",
  "author": "Harinarayanan K P harikpaug18@gmail.com",
  "version": "2.0.4",
  "description": "A modern, lightweight React UI component library with built-in theming, accessibility, and full TypeScript support. Create beautiful, responsive, and customizable web apps faster with ready-to-use components for any project.",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://tharikida-ui.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/harinarayanan-kp/tharikida-ui"
  },
  "keywords": [
    "UI",
    "React",
    "component library",
    "React components",
    "Tharikida UI",
    "UI kit",
    "design system",
    "typescript",
    "customizable",
    "component",
    "frontend",
    "web",
    "library",
    "open source",
    "theme",
    "modern",
    "responsive"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./button": {
      "import": "./dist/button.js",
      "require": "./dist/button.js",
      "types": "./dist/button.d.ts"
    },
    "./datadisplay": {
      "import": "./dist/datadisplay.js",
      "require": "./dist/datadisplay.js",
      "types": "./dist/datadisplay.d.ts"
    },
    "./input": {
      "import": "./dist/input.js",
      "require": "./dist/input.js",
      "types": "./dist/input.d.ts"
    },
    "./navigation": {
      "import": "./dist/navigation.js",
      "require": "./dist/navigation.js",
      "types": "./dist/navigation.d.ts"
    },
    "./specials": {
      "import": "./dist/specials.js",
      "require": "./dist/specials.js",
      "types": "./dist/specials.d.ts"
    },
    "./media": {
      "import": "./dist/media.js",
      "require": "./dist/media.js",
      "types": "./dist/media.d.ts"
    },
    "./feedback": {
      "import": "./dist/feedback.js",
      "require": "./dist/feedback.js",
      "types": "./dist/feedback.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "start": "npm link",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}
